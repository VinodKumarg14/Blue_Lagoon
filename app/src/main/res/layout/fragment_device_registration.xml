<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/popup_bg"
    android:layout_width="match_parent"
    android:padding="0dp"
    android:layout_height="match_parent">
     <include layout="@layout/dialog_header_fragment" />
    <ScrollView
        android:layout_width="match_parent"
        android:fillViewport="true"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="80px">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_reg_edit_text"
                android:layout_marginBottom="@dimen/margin_device_reg_top_bottom"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="DeviceId"
                    android:textSize="@dimen/text_size_18"
                    android:textColor="@color/black"
                    />
                <EditText
                    android:id="@+id/etDeviceId"
                    android:layout_marginLeft="80px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:background="@drawable/cancel"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                     android:layout_marginRight="50px"/>
                <TextView
                    android:textSize="@dimen/text_size_18"
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="Tablet Model"
                    android:textColor="@color/black"/>
                <EditText
                    android:id="@+id/etTableModel"
                    android:layout_marginLeft="120px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@drawable/cancel"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_reg_edit_text"
                android:layout_marginTop="@dimen/margin_device_reg_top_bottom"
                android:layout_marginBottom="@dimen/margin_device_reg_top_bottom"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="IMEI 1"
                    android:textSize="@dimen/text_size_18"
                    android:textColor="@color/black"
                    />
                <EditText
                    android:id="@+id/etIMEI1"
                    android:layout_marginLeft="80px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@drawable/cancel"
                    android:layout_marginRight="50px"/>
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="IMEI 2"
                    android:textSize="@dimen/text_size_18"
                    android:textColor="@color/black"/>
                <EditText
                    android:id="@+id/etIMEI2"
                    android:layout_marginLeft="120px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@drawable/cancel"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_reg_edit_text"
                android:layout_marginTop="@dimen/margin_device_reg_top_bottom"
                android:layout_marginBottom="@dimen/margin_device_reg_top_bottom"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="SIM\nNumber"
                    android:textSize="@dimen/text_size_18"
                    android:textColor="@color/black"
                    />
                <EditText
                    android:id="@+id/etSimNum"
                    android:layout_marginLeft="80px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@drawable/cancel"
                    android:layout_marginRight="50px"/>
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="Mobile\nNumber"
                    android:textSize="@dimen/text_size_18"
                    android:textColor="@color/black"/>
                <EditText
                    android:id="@+id/etMobileNumber"
                    android:layout_marginLeft="120px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@drawable/cancel"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_reg_edit_text"
                android:layout_marginTop="@dimen/margin_device_reg_top_bottom"
                android:layout_marginBottom="@dimen/margin_device_reg_top_bottom"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="Register \nEmployee ID"
                    android:textSize="@dimen/text_size_18"
                    android:textColor="@color/black"
                    />
                <EditText
                    android:id="@+id/etEmpId"
                    android:layout_marginLeft="80px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@drawable/cancel"
                    android:layout_marginRight="50px"/>
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="Available\nMemory"
                    android:textSize="@dimen/text_size_18"
                    android:textColor="@color/black"/>
                <EditText
                    android:id="@+id/etAvailbleMemory"
                    android:layout_marginLeft="120px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="match_parent"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@drawable/cancel"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_reg_edit_text"
                android:layout_marginTop="@dimen/margin_device_reg_top_bottom"
                android:layout_marginBottom="@dimen/margin_device_reg_top_bottom"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="Address"
                    android:textSize="@dimen/text_size_18"
                    android:textColor="@color/black"
                    />
                <EditText
                    android:id="@+id/etAddress"
                    android:layout_marginLeft="80px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionDone"
                    android:singleLine="true"
                    android:background="@drawable/cancel"
                    android:layout_marginRight="50px"/>
                <TextView
                    android:layout_width="@dimen/device_reg_label_width"
                    android:layout_height="wrap_content"
                    android:text="Available\n Memory"
                    android:visibility="invisible"
                    android:textColor="@color/black"/>
                <EditText
                    android:layout_marginLeft="120px"
                    android:layout_width="@dimen/width_reg_edit_text"
                    android:layout_height="@dimen/height_reg_edit_text"
                    android:paddingLeft="@dimen/padding_left_edit_text"
                    android:paddingRight="@dimen/padding_left_edit_text"
                    android:imeOptions="actionNext"
                    android:singleLine="true"
                    android:background="@drawable/cancel"
                    android:visibility="invisible"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">
                <TextView
                    android:id="@+id/tvRegistration"
                    android:layout_width="210px"
                    android:layout_height="64px"
                    android:background="@drawable/confirm_selector"
                    android:gravity="center"
                    android:paddingLeft="@dimen/dp_10"
                    android:paddingTop="@dimen/dp_5"
                    android:paddingRight="@dimen/dp_10"
                    android:paddingBottom="@dimen/dp_5"
                    android:text="Register"
                    android:textColor="@color/white"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tvClear"
                    android:layout_width="210px"
                    android:layout_height="64px"
                    android:layout_marginLeft="@dimen/dp_30"
                    android:gravity="center"
                    android:paddingLeft="@dimen/dp_10"
                    android:paddingTop="@dimen/dp_5"
                    android:paddingRight="@dimen/dp_10"
                    android:paddingBottom="@dimen/dp_5"
                    android:text="Clear"
                    android:textColor="@color/black"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="600px"
             ></LinearLayout>


        </LinearLayout>


    </ScrollView>



</LinearLayout>